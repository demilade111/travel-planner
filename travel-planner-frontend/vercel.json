{
  "version": 2,
  "buildCommand": "cd travel-planner-frontend && npm install --force && npm run build",
  "outputDirectory": "travel-planner-frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
} 